        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="me.batizhao.dp.mapper.FormMapper">

  <resultMap id="formMap" type="me.batizhao.dp.domain.Form">
    <id property="id" column="id"/>
